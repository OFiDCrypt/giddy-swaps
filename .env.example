# üîê Wallet
# Path to your Solana keypair JSON file (e.g., for signing transactions)
KEYPAIR_PATH=YOUR_KEYPAIR_PATH_HERE  # e.g., /path/to/your/mint-authority.json

# ü™ô Token mints
# USDC mint address on Solana (standard)
USDC_MINT=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
# Your custom token mint (e.g., GIDDY)
GIDDY_MINT=YOUR_GIDDY_MINT_HERE  # e.g., 8kQzvMELBQGSiFmrXqLuDSpYVLKkNoXE4bUQCC14wj3Z

# üåê RPC configuration
# Solana RPC endpoint (mainnet-beta for production)
RPC_URL=YOUR_RPC_URL_HERE  # e.g., https://api.mainnet-beta.solana.com
# Number of RPC retries on failure
RPC_RETRIES=5
# RPC request timeout in ms
RPC_TIMEOUT=30000
# Note: Public RPC may cause timeouts or rate limits. Consider a paid RPC (e.g., Helius).

# üîÑ Swap parameters
# Base swap amount in raw units (e.g., 100000 for 0.1 USDC with 6 decimals)
SWAP_AMOUNT=100000
# Slippage tolerance in basis points (e.g., 200 = 2%)
SLIPPAGE_BPS=200
# Minimum output amount (raw units; 0 to disable)
MIN_OUT_AMOUNT=0
# Interval between swaps in seconds
SWAP_INTERVAL=300
# Initial deposit amount in human-readable USDC (e.g., 10)
INITIAL_AMOUNT=10
# Minimum USDC threshold to continue swaps
MIN_SWAP_AMOUNT=0.01
# Max USDC to use per buy swap
MAX_BUY_USDC=10
# Start direction: 'forward' (buy) or 'backward' (sell)
INITIAL_DIRECTION=forward
# Good: 0.1 USDC swap amount (raw units), 2% slippage, 300s (5 min) interval.
# INITIAL_AMOUNT/MIN_SWAP_AMOUNT: Human-readable (e.g., 10 USDC min start, 0.01 stop threshold).
# MAX_BUY_USDC: Max USDC to swap on buy (full if less; defaults to 10).

# ‚öôÔ∏è DLMM Fallback (optional)
# Enable DLMM as fallback after Jupiter fails
USE_DLMM_FALLBACK=false
# Set to 'true' to enable direct DLMM after Jupiter fails.

# üì° Telegram Alerts
# Your Telegram bot token
TELEGRAM_TOKEN=YOUR_TELEGRAM_TOKEN_HERE  # e.g., 1234567890:AAEh... (from @BotFather)
# Your Telegram chat ID for alerts (user/group ID)
TELEGRAM_CHAT_ID=YOUR_CHAT_ID_HERE  # e.g., 123456789 (get via @userinfobot)

# ü§ñ GEMMA 3 AI Assistant -Hugging Face | Nebius (optional)
# Hugging Face API token for Gemma model access
HF_TOKEN=YOUR_HF_TOKEN_HERE  # e.g., hf_... (from huggingface.co/settings/tokens)
# Nebius API key for advanced inference (optional alternative to HF)
NEBIUS_API_KEY=YOUR_NEBIUS_API_KEY_HERE  # e.g., eyJhbGciOi... (from nebius.ai)